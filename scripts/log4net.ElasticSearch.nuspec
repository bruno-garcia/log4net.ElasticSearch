        </dependencies>
    </metadata>
    <files>
        <file src="lib\**\log4net.ElasticSearch*.dll" target="lib"/>
        <file src="lib\**\log4net.ElasticSearch*.pdb" target="lib"/>
        <file src="lib\**\log4net.ElasticSearch*.XML" target="lib"/>
        <file src="readme.txt" target="" />
